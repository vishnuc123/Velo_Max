<div id="editCategoryModal" class="fixed inset-0 flex items-center justify-center bg-black bg-opacity-50 hidden z-50">
  <div class="bg-white rounded-lg shadow-lg w-1/3">
    <div class="px-4 py-2 border-b border-gray-300">
      <h3 class="text-xl font-bold">Edit Category</h3>
    </div>
    <div class="p-4">
      <form id="editCategoryForm" enctype="multipart/form-data">
        <input type="hidden" id="editCategoryId" />
        
        <div class="mb-4">
          <label class="block text-sm font-medium text-gray-700 mb-1" for="editCategoryTitle">Title</label>
          <input 
            type="text" 
            id="editCategoryTitle" 
            class="w-full border border-gray-300 rounded-lg px-3 py-2 focus:outline-none focus:ring-2 focus:ring-blue-500" 
            readonly
          />
        </div>
        
        <div class="mb-4">
          <label class="block text-sm font-medium text-gray-700 mb-1" for="editCategoryDescription">Description</label>
          <textarea 
            id="editCategoryDescription" 
            class="w-full border border-gray-300 rounded-lg px-3 py-2 focus:outline-none focus:ring-2 focus:ring-blue-500"
          ></textarea>
        </div>
        
        <!-- Image Management Section -->
        <div class="mb-4">
          <label class="block text-sm font-medium text-gray-700 mb-1">Current Image</label>
          <div class="flex items-center">
            <img 
              id="currentCategoryImage" 
              src="" 
              alt="Category Image" 
              class="w-24 h-24 rounded-lg object-cover border border-gray-300"
            />
            <button 
              type="button" 
              id="removeImageButton" 
              class="ml-4 px-4 py-2 bg-red-500 text-white font-medium rounded-lg hover:bg-red-600"
            >
              Remove
            </button>
          </div>
        </div>
        
        <div class="mb-4">
          <label class="block text-sm font-medium text-gray-700 mb-1" for="editCategoryImage">Replace Image</label>
          <input 
            type="file" 
            id="editCategoryImage" 
            accept="image/*" 
            class="w-full border border-gray-300 rounded-lg px-3 py-2 focus:outline-none focus:ring-2 focus:ring-blue-500"
          />
        </div>
        
        <div class="flex justify-end">
          <button 
