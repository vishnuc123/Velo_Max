<div id="modal" class="modal absolute inset-0 bg-gray-800 bg-opacity-50 flex items-center justify-center overflow-auto mt-5 hidden">
  <div class="modal-content bg-white rounded-lg p-6 shadow-lg w-11/12 md:w-1/3 max-h-screen overflow-auto">
      <h2 class="text-xl font-semibold mb-4">Add New Category</h2>
      <form action="/category" id="categoryForm" method="post" enctype="multipart/form-data">
  
          <label class="block mb-2" for="categoryName">Category Name</label>
          <input type="text" id="categoryName" class="border border-gray-300 rounded-md p-2 w-full mb-4" name="categoryName">
          <div id="categoryNameError"></div>

          <label class="block mb-2" for="categoryDescription">Description</label>
          <textarea id="categoryDescription" class="border border-gray-300 rounded-md p-2 w-full mb-4" name="category-Description"></textarea>
          <div id="categoryDescError"></div>
        
          

          <label class="block mb-2" for="categoryImage">Category Image</label>
          <div id="wrapper" class=" w-half">
            
          </div>
          <input type="file" id="input" accept=".jpg, .jpeg," class="mb-4" name="category-image">
          <div id="categoryImageError"></div>
          <!-- button for cropping -->
          <button class="bg-green-500 text-white font-semibold py-2 px-4 rounded hover:bg-green-600 transition ease-in-out duration-300 hidden" id="crop-button">
            Crop
        </button>

        <button id="infoSpecificationButton" class="relative left-0 bg-gradient-to-r from-indigo-500 via-purple-500 to-pink-500 text-black font-bold py-2 px-6 rounded-lg shadow-lg transform hover:scale-105 transition-all duration-300 ease-in-out hover:shadow-2xl">
          ⚠️About custom Specification
        </button>
        <!-- Modal for cropping -->
