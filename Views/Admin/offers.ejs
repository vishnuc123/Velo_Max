        <div class="container mx-auto p-8">
          <h1
            class="text-4xl font-bold text-center text-gray-800 mb-8 animate-float"
          >
            Offer Management
          </h1>

          <div class="flex justify-end mb-6">
            <button
              id="openModalBtn"
              class="bg-gradient-to-r from-gray-500 to-gray-700 hover:from-gray-600 hover:to-gray-800 text-black font-bold py-2 px-6 rounded-full transition duration-300 ease-in-out transform hover:scale-105 focus:outline-none focus:ring-2 focus:ring-gray-500 focus:ring-opacity-50"
            >
              <i class="fas fa-plus-circle mr-2"></i> Add Offer
            </button>
          </div>

          <form
            id="offerForm"
            class="hidden bg-white shadow-2xl rounded-lg p-8 mb-8 transition-all duration-500 ease-in-out transform scale-95 opacity-0"
          >
            <h2
              class="text-3xl font-bold mb-6 text-center text-gray-800 animate-float"
            >
              Create New Offer
            </h2>

            <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
              <!-- Offer Name -->
              <div>
                <label
                  class="block text-gray-700 text-sm font-bold mb-2"
                  for="offer-name"
                >
                  Offer Name
                </label>
                <input
                  class="shadow appearance-none border rounded-lg w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:ring-2 focus:ring-gray-500"
                  id="offer-name"
                  type="text"
                  placeholder="Enter offer name"
                />
                <div
                  id="offer-name-error"
                  class="text-red-500 text-sm hidden"
                ></div>
              </div>

              <!-- Offer Type -->
              <div>
                <label
                  class="block text-gray-700 text-sm font-bold mb-2"
                  for="offer-type"
                >
                  Offer Type
                </label>
                <select
                  class="shadow appearance-none border rounded-lg w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:ring-2 focus:ring-gray-500"
                  id="offer-type"
                >
                  <option value="">Select Offer Type</option>
                  <option value="category">Category-based</option>
                  <option value="product">Product-based</option>
                </select>
                <div
                  id="offer-type-error"
                  class="text-red-500 text-sm hidden"
                ></div>
              </div>

              <!-- Category Select -->
              <div id="category-select" class="hidden">
                <label
                  class="block text-gray-700 text-sm font-bold mb-2"
                  for="category"
                >
                  Category
                </label>
                <select
                  class="shadow appearance-none border rounded-lg w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:ring-2 focus:ring-gray-500"
                  id="category"
                >
                  <option value="">Select Category</option>
                  <option value="electronics">Electronics</option>
                  <option value="clothing">Clothing</option>
                  <option value="books">Books</option>
                </select>
                <div
                  id="category-error"
                  class="text-red-500 text-sm hidden"
                ></div>
              </div>

              <!-- Product Search -->
              <div id="product-search" class="hidden">
                <label
                  class="block text-gray-700 text-sm font-bold mb-2"
                  for="product"
                >
                  Search Product
                </label>
                <div class="relative">
                  <input
                    type="text"
                    id="product-search-input"
                    class="shadow appearance-none border rounded-lg w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:ring-2 focus:ring-gray-500"
                    placeholder="Search for a product..."
                  />
                  <div
                    class="absolute inset-y-0 right-0 flex items-center pr-3"
                  >
                    <i class="fas fa-search text-gray-400"></i>
                  </div>
                </div>
                <div
                  id="product-search-results"
                  class="mt-2 bg-white border border-gray-300 rounded-lg shadow-lg hidden"
                ></div>
                <div
                  id="product-error"
                  class="text-red-500 text-sm hidden"
                ></div>
              </div>

              <!-- Discount Type -->
              <div>
                <label
                  class="block text-gray-700 text-sm font-bold mb-2"
                  for="discount-type"
                >
                  Discount Type
                </label>
                <select
                  class="shadow appearance-none border rounded-lg w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:ring-2 focus:ring-gray-500"
                  id="discount-type"
                >
                  <option value="">Select Discount Type</option>
                  <option value="percentage">Percentage Discount</option>
                  <option value="fixed">Fixed Amount Discount</option>
                </select>
                <div
                  id="discount-type-error"
                  class="text-red-500 text-sm hidden"
                ></div>
              </div>

              <!-- Discount Value -->
              <div>
                <label
                  class="block text-gray-700 text-sm font-bold mb-2"
                  for="discount-value"
                >
                  Discount Value
                </label>
                <input
                  class="shadow appearance-none border rounded-lg w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:ring-2 focus:ring-gray-500"
                  id="discount-value"
                  type="number"
                  placeholder="Enter discount value"
                />
                <div
                  id="discount-value-error"
                  class="text-red-500 text-sm hidden"
                ></div>
              </div>

              <!-- Start Date -->
              <div>
                <label
                  class="block text-gray-700 text-sm font-bold mb-2"
                  for="start-date"
                >
                  Start Date
                </label>
                <input
                  class="shadow appearance-none border rounded-lg w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:ring-2 focus:ring-gray-500"
                  id="start-date"
                  type="date"
                />
                <div
                  id="start-date-error"
                  class="text-red-500 text-sm hidden"
                ></div>
              </div>

              <!-- End Date -->
              <div>
                <label
                  class="block text-gray-700 text-sm font-bold mb-2"
                  for="end-date"
                >
                  End Date
                </label>
                <input
                  class="shadow appearance-none border rounded-lg w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:ring-2 focus:ring-gray-500"
                  id="end-date"
                  type="date"
                />
                <div
                  id="end-date-error"
                  class="text-red-500 text-sm hidden"
                ></div>
              </div>
            </div>

            <div class="flex items-center justify-between mt-8">
              <button
                id="createOfferButton"
                class="bg-gradient-to-r from-green-400 to-green-600 hover:from-green-500 hover:to-green-700 text-black font-bold py-2 px-6 rounded-full transition duration-300 ease-in-out transform hover:scale-105 focus:outline-none focus:ring-2 focus:ring-green-500 focus:ring-opacity-50"
              >
                <i class="fas fa-check-circle mr-2"></i> Create Offer
              </button>
              <button
                id="cancelButton"
                class="bg-gradient-to-r from-red-400 to-red-600 hover:from-red-500 hover:to-red-700 text-black font-bold py-2 px-6 rounded-full transition duration-300 ease-in-out transform hover:scale-105 focus:outline-none focus:ring-2 focus:ring-red-500 focus:ring-opacity-50"
              >
                <i class="fas fa-times-circle mr-2"></i> Cancel
              </button>
            </div>
          </form>

          <div class="bg-white shadow-2xl rounded-lg overflow-hidden">
            <div class="overflow-x-auto">
              <table class="w-full">
                <thead>
                  <tr
                    class="bg-gradient-to-r from-gray-600 to-gray-800 text-black"
                  >
                    <th
                      class="py-4 px-6 text-left font-semibold uppercase tracking-wider"
                    >
                      Offer Name
                    </th>
                    <th
                      class="py-4 px-6 text-left font-semibold uppercase tracking-wider"
                    >
                      Type
                    </th>
                    <th
                      class="py-4 px-6 text-left font-semibold uppercase tracking-wider"
                    >
                      Discount
                    </th>
                    <th
                      class="py-4 px-6 text-left font-semibold uppercase tracking-wider"
                    >
                      Start Date
                    </th>
                    <th
                      class="py-4 px-6 text-left font-semibold uppercase tracking-wider"
                    >
                      End Date
                    </th>
                    <th
                      class="py-4 px-6 text-left font-semibold uppercase tracking-wider"
                    >
                      Status
                    </th>
                    <th
                      class="py-4 px-6 text-left font-semibold uppercase tracking-wider"
                    >
                      Action
                    </th>
                  </tr>
                </thead>
                <tbody id="offer-list" class="divide-y divide-gray-200"></tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
      <!-- end content -->
    </div>
