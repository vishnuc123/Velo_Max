    <div class="container mx-auto p-4">
        <div class="flex justify-end w-25 h-10">
            <div id="product-message"></div>
            <button class="bg-gray-600 hover:bg-gray-800 text-white font-bold py-2 px-4 rounded-full" id="openModalBtn">
                Add Coupon
            </button>
        </div>

        <form id="couponForm" class="bg-white shadow-md rounded-lg px-8 pt-6 pb-8 mb-4 animate-fadeIn" style="display: none;">
          <h2 class="text-2xl font-bold mb-6 text-center text-gray-800 animate-float">Create New Coupon</h2>
      
          <div class="grid grid-cols-2 gap-6">
              <!-- Left Column -->
              <div>
                  <div class="mb-4">
                      <label class="block text-gray-700 text-sm font-bold mb-2" for="coupon-name">Coupon Name</label>
                      <input class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="coupon-name" type="text" placeholder="Enter coupon name">
                      <p class="text-red-500 text-xs italic hidden" id="coupon-name-error">Please enter a coupon name.</p>
                  </div>
      
                  <div class="mb-4">
                      <label class="block text-gray-700 text-sm font-bold mb-2" for="coupon-code">Coupon Code</label>
                      <input class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="coupon-code" type="text" placeholder="Enter coupon code">
                      <p class="text-red-500 text-xs italic hidden" id="coupon-code-error">Please enter a coupon code.</p>
                  </div>
      
                  <div class="mb-4">
                      <label class="block text-gray-700 text-sm font-bold mb-2" for="usage-limit">Usage Limit</label>
                      <input class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="usage-limit" type="number" placeholder="Enter usage limit">
                      <p class="text-red-500 text-xs italic hidden" id="usage-limit-error">Please enter a valid usage limit.</p>
                  </div>
      
                  <div class="mb-4">
                      <label class="block text-gray-700 text-sm font-bold mb-2" for="usage-limit-per-user">Usage Limit per user</label>
                      <input class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="usage-limit-per-user" type="number" placeholder="Enter usage limit">
                      <p class="text-red-500 text-xs italic hidden" id="usage-limit-per-user-error">Please enter a valid usage limit.</p>
                  </div>
      
                  <div class="mb-4">
                      <label class="block text-gray-700 text-sm font-bold mb-2" for="discount-type">Discount Type</label>
                      <select class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="discount-type">
                          <option value="">Select Discount Type</option>
                          <option value="percentage">Percentage Discount</option>
                          <option value="fixed">Fixed Amount Discount</option>
                      </select>
                      <p class="text-red-500 text-xs italic hidden" id="discount-type-error">Please select a discount type.</p>
                  </div>
              </div>
      
              <!-- Right Column -->
              <div>
                  <div class="mb-4">
                      <label class="block text-gray-700 text-sm font-bold mb-2" for="discount-amount">Discount Amount</label>
                      <input class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="discount-amount" type="number" placeholder="Enter discount amount">
                      <p class="text-red-500 text-xs italic hidden" id="discount-amount-error">Please enter a valid discount amount.</p>
                  </div>
      
                  <div class="mb-4">
                      <label class="block text-gray-700 text-sm font-bold mb-2" for="min-price">Minimum Price</label>
                      <input class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="min-price" type="number" placeholder="Enter minimum price">
                      <p class="text-red-500 text-xs italic hidden" id="min-price-error">Please enter a valid minimum price.</p>
                  </div>
      
                  <div class="mb-4">
                      <label class="block text-gray-700 text-sm font-bold mb-2" for="max-price">Maximum Price</label>
                      <input class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="max-price" type="number" placeholder="Enter maximum price">
                      <p class="text-red-500 text-xs italic hidden" id="max-price-error">Maximum price must be greater than minimum price.</p>
                  </div>
      
                  <div class="mb-4">
                      <label class="block text-gray-700 text-sm font-bold mb-2" for="start-date">Start Date</label>
                      <input class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="start-date" type="date">
                      <p class="text-red-500 text-xs italic hidden" id="start-date-error">Please enter a start date.</p>
                  </div>
      
                  <div class="mb-4">
                      <label class="block text-gray-700 text-sm font-bold mb-2" for="end-date">End Date</label>
                      <input class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="end-date" type="date">
                      <p class="text-red-500 text-xs italic hidden" id="end-date-error">End date must be after start date.</p>
                  </div>
              </div>
          </div>
      
          <div class="flex items-center justify-between mt-6">
